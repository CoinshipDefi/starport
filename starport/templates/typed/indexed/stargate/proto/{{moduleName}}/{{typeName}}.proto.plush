syntax = "proto3";
package <%= nodash(OwnerName) %>.<%= AppName %>.<%= ModuleName %>;

option go_package = "<%= ModulePath %>/x/<%= ModuleName %>/types";

import "gogoproto/gogo.proto";

message <%= title(TypeName) %> {
  <%= for (i, field) in Fields { %>
  <%= field.Datatype %> <%= field.Name %> = <%= i+2 %>; <% } %>
}
